{ 
    "extends": "airbnb",
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "rules": {
          "linebreak-style": "off", 
        "indent": "off",  
        "quotes": "off",
        "semi": "off",
        "no-trailing-spaces": "off", 
        "no-multiple-empty-lines": "off", 
        "no-param-reassign": "off", 
        "no-console": "off",
        "import/no-unresolved": "off",
        "import/no-extraneous-dependencies": "off"
    }
  } 